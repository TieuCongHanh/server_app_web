            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
            <th>Desc</th>
            <th>theloai</th>
        </tr>

        <% listSP.forEach((row) => { %>
        <tr>
            <td><a href="sp/edit/<%= row._id %>"><%= row._id %></a></td>
            
